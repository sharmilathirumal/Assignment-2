package assignmentTwo;

public class Person {
	  String employeeName;
       int employeeAge;
}

package assignmentTwo;

public class Employee extends Person{
	int employeeId;
    double employeeSalary;
    public Employee(String name,int age,int id,double salary){
    	super.employeeName = name;
        super.employeeAge = age;
        this.employeeId =id;
        this.employeeSalary = salary;
    }
    
    public static void main(String []args){
      Employee employee = new Employee("Iyal", 25, 622, 50000.0);
       System.out.println("Name: " +employee.employeeName);
       System.out.println("Age: " + employee.employeeAge);
       System.out.println("Employee ID: " + employee.employeeId);
       System.out.println("Employee Salary: " + employee.employeeSalary);
   
   }
	
}
